  </head>
  <body>
  
    <div id='header'>
      <h1> TIC TAC TOE </h1>
    </div>

    <div id='main'>
      <%= board %>
    </div>


    <div class=board id=board>
      <table id=MyTable>
          <tr>
              <td >  <%= board[0][0] %> <form method="post" action="/make-move/1" > <input type="text" name="mark"/> </form> </td> 
