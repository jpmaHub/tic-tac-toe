              <td >  <%= board[1][2] %> <form method="post" action="/make-move/6"> <input type="text" name="mark"/> </form> </td>
        
              <td >  <%= board[2][0] %> <form method="post" action="/make-move/7"> <input type="text" name="mark"/> </form> </td>
              <td >  <%= board[2][1] %> <form method="post" action="/make-move/8"> <input type="text" name="mark"/> </form> </td>
              <td >  <%= board[2][2] %> <form method="post" action="/make-move/9"> <input type="text" name="mark"/> </form> </td>
          </tr>
      </table>
